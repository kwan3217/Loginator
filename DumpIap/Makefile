iap_map.arm.o: iap_map.hex Makefile
	arm-none-eabi-objcopy $< -O elf32-little -B arm --rename-section .sec1=.arm --set-section-flags .sec1=alloc,contents,load,code,rom $(ARM_SYMBOLS) $@
